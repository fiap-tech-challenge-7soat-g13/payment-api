{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "required": [
    "id",
    "qrCode",
    "status"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "qrCode": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": ["PENDENTE", "APROVADO", "FALHADO"]
    }
  }
}
